{
  "version": 2,
  "name": "akassect-whitepaper",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/ko",
      "dest": "/ko/index.html"
    },
    {
      "src": "/ko/(.*)",
      "dest": "/ko/$1"
    },
    {
      "src": "/en",
      "dest": "/en/index.html"
    },
    {
      "src": "/ja",
      "dest": "/ja/index.html"
    },
    {
      "src": "/zh",
      "dest": "/zh/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/whitepaper",
      "destination": "/ko/",
      "permanent": true
    }
  ]
}
